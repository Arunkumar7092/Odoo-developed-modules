<odoo>
    <template id="employee_thanks" name="employeethanks">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <div id="IndexSubmit">
                    <div class="container">
                        <h1>Thanks!</h1>

                        <form action="/employee_webform" method="POST">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>     
                            
                                <div class="row">
                                    <div class="col-lg-8">
                                        <div class="alert alert-success" role="status"> Form Submitted  successfully . </div>
                                        
                                    </div>
                                </div>
                                <div class="modal-footer">

                                    <button type="submit"  class="btn btn-primary">Dashboard</button>
                                    <button padding = "15px 42px" type="button" class="btn btn-primary pull-center">Submit</button>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
        </t>
    </template>
</odoo>