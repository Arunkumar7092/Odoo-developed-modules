<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <report
        id="report_emmployee_details"
        string="employee previous work details"
        model="arun.test"
        report_type="qweb-pdf"
        name="arun_test.employee_details"
        file="arun_test.employee_details"/>

    
    <report
        id="report_employee_appointment_letter"
        string="Appointment letter"
        model="arun.test"
        report_type="qweb-pdf"
        name="arun_test.employee_appointment_letter"
        file="arun_test.employee_appointment_letter"/>
    <report
        id="report_employee_resume"
        string="Resume letter"
        model="arun.test"
        report_type="qweb-pdf"
        name="arun_test.employee_resume"
        file="arun_test.employee_resume"/>
    
     <record id="paperformat_resume_report" model="report.paperformat">
        <field name="name">European A4</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">20</field>
        <field name="margin_left">4</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>
    <record id="arun_test.report_employee_resume" model="ir.actions.report">

        <field name="paperformat_id" ref="arun_test.paperformat_resume_report"/>

    </record>
    

   
    
</odoo>

