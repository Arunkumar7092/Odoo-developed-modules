<odoo>
    <template id="employee_resume">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.basic_layout">
                
                    <div class="header">
                        <center><h>Resume</h></center>
                        <right><span t-esc="context_timestamp(datetime.datetime.now()).strftime('%d/%m/%Y')"/></right>
                    </div>
                    <table width="100%">
                        <tr>
                            <td width="50%">
                            <img class="left" style="max-height:150px;max-width:250px;"  src='arun_test/static/description/icon1.png'/>
                            </td>
                            <td style="font-size:15px;font-family:sans-serif;text-align:left;font-weight:bold;" width="50%">
                            <span t-esc="o.test_name"/></td>
                        </tr>
                        <tr>
                            <td width="60%">
                            </td>
                            <td style="font-size:15px;font-family:sans-serif;text-align:left;font-weight:bold;" width="30%"> 
                            <span t-esc="o.job_position"/></td>
                            
                        </tr>
                        <tr>
                            <td width="80%" style="text-align:left;font-size:18px;font-family:sans-serif;"><strong>CONTACT</strong></td>
                        </tr><br/><br/><br/>
                        <tr>
                            <td width="80%" style="text-align:left;font-size:18px;font-family:sans-serif;">
                                <span t-field="o.inst_owner"/>
                            </td>
                        </tr>
                        <tr>
                            <td width="80%" style="text-align:left;font-size:18px;font-family:sans-serif;">
                                <span t-field="o.company_name"/>
                            </td>
                        </tr>
                        <tr>
                            <td width="80%" style="text-align:left;font-size:18px;font-family:sans-serif;">
                                <span t-field="o.email_id"/>
                            </td>
                        </tr>
                    </table>


                </t>
            </t>
        </t>
    </template>
</odoo>
